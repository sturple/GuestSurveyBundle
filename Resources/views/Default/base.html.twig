<body>
	<div class="header">
		<div class="column-wrapper">
			<div class="row">
				<div class="col-xs-6"><img style="" class="" src="{{images_path}}{{config.survey.images.logos.white}}" alt="{{property.name}}" /></div>
				{% if config.survey.images.logos.group %}
				<div class="col-xs-6" style="text-align: right;"><img style="" class="" src="{{images_path}}{{config.survey.images.logos.group}}" alt="{{reporting.group.name}}  Logo" /></div>
				{% endif %}
			</div>
		</div>
	</div>
	<div class="container">
		<div class="main">
			<div class="column-wrapper">
				<div class="row">
					<div class="col-sm-12">	{%block body %}{%endblock%}	</div>
				</div>
			</div>
		</div>
	</div>
	<div class="footer">
		<div class="column-wrapper">
			{%block footer %}{% endblock %}
		</div>
	</div>
	
	
	<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
